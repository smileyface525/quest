<!DOCTYPE html>
<html>
<head>
  <title>Quest</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <header>
    <h1>Quest</h1>
    <% if logged_in? %>
      <%= link_to 'logout', session_path, method: :delete %>
    <% end %>
  </header>

<%= yield %>

</body>
</html>
